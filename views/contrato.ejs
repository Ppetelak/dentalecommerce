<body>
    <div class="topo sticky-top">
            <h2> Prévia do documento </h2>
            <button type="button" class="btn btn-primary salvar" id="gerar-pdf"> IMPRIMIR/ SALVAR PDF </button>
    </div>
    <main>
        <header class="header">
            <div class="row align-items-center">
                <div class="col-6">
                    <div class="row">
                        <div class="col-sm-6">
                            <img class="mw-100" src="/img/logomounthermonoriginal.png">
                        </div>
                        <div class="col-sm-6">
                            <img class="mw-100" src="<%= plano.logo %>">
                        </div>
                    </div>
                </div>
                <div class="col-6">
                    <div class="row infos">
                        <div class="col-4">
                            Proposta <br>
                            <span class="valores">
                                Teste de informação
                            </span>
                        </div>
                        <div class="col-4">
                            Data Criação <br>
                            <span class="valores">
                                <%= dataFormatada %>
                            </span>
                        </div>
                    </div>
                </div>
            </div>
        </header>
        <div class="content">
            <div class="row dados titular">
                <div class="col-sm-12">
                    <h1>Contratante</h1>
                    <div class="row">
                        <div class="col-sm-6">
                            <p>CPF: <span> <%= implantacao.cpftitular %></span></p>
                        </div>
                        <div class="col-sm-6">
                            <p>RG: <span> <%= implantacao.rgtitular %> </span></p>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-6">
                            <p>Nome completo: <span> <%= implantacao.nomecompleto %></span></p>
                        </div>
                        <div class="col-sm-6">
                            <p>Data de Nascimento: <span> <%= implantacao.datanascimento %> </span></p>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-12">
                            <p>Endereço Completo: <span> <%= implantacao.enderecoresidencial %>, <%= implantacao.numeroendereco %>, <%= implantacao.bairro %>, <%= implantacao.cidade %> - <%= implantacao.estados %>, CEP: <%= implantacao.cep %></span></p>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-6">
                            <p>Nome da Mãe: <span> <%= implantacao.nomemaetitular %></span></p>
                        </div>
                        <div class="col-sm-6">
                            <p>Sexo: <span> <%= implantacao.sexotitular %> </span></p>
                        </div>
                    </div>
                    <div class="row">
